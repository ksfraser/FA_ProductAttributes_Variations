{
    "name": "ksfraser/fa-product-attributes-variations",
    "description": "Product Variations plugin for FA_ProductAttributes",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "path",
            "url": "../"
        },
        {
            "type": "path",
            "url": "../famock"
        }
    ],
    "require": {
        "php": ">=7.3",
        "ksfraser/fa-product-attributes": "*",
        "ksfraser/famock": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.0"
    },
    "autoload": {
        "psr-4": {
            "Ksfraser\\FA_ProductAttributes_Variations\\": "src/Ksfraser/FA_ProductAttributes_Variations/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ksfraser\\FA_ProductAttributes_Variations\\Test\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "platform": {
            "php": "7.3.21"
        }
    }
}
